{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:sonarjs/recommended",
    "plugin:unicorn/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": ["emotion", "sonarjs", "unicorn", "import"],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "semi": 1
  },
  "settings": {
    "importer/resolver": "webpack"
  }
}