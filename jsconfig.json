{
  "compilerOptions": {
    "jsx": "react",
    "sourceMap": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@img/*": [
        "./src/assets/img/*"
      ],
      "@fonts/*": [
        "./src/assets/fonts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@/*": [
        "./src/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ],
  "include": [
    "./src"
  ]
}