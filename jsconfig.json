{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@img/*": [
        "./assets/img/*"
      ],
      "@fonts/*": [
        "./assets/fonts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ],
  "include": [
    "./src",
    "./assets"
  ]
}